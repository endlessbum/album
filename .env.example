# Example environment variables for local development
# Copy to .env and adjust values.

# PostgreSQL connection string (leave empty to use in-memory MemStorage in dev)
DATABASE_URL=

# Session secret for express-session (use a long random string)
SESSION_SECRET=dev_secret_change_me

# Public origin of the app (used for cookies/callbacks)
APP_ORIGIN=http://127.0.0.1:5001

# Server port (dev picks next free if busy)
PORT=5001
